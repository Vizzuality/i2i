{
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "mocha": true
  },
  "globals": {
    "google": true,
    "L": true
  },
  "extends": "vizzuality",
  "parser": "babel-eslint",
  "rules": {
    "no-debugger": 0,
    "class-methods-use-this": 0,
    "jsx-a11y/click-events-have-key-events": 0
  },
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  }
}